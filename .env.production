# Production environment configuration
VITE_API_BASE_URL=https://api.ai-bpms.example.com/api/v1
VITE_WEBSOCKET_URL=wss://api.ai-bpms.example.com/ws

# Authentication (Keycloak) - Production
VITE_KEYCLOAK_URL=https://auth.ai-bpms.example.com/auth
VITE_KEYCLOAK_REALM=ai-bpms
VITE_KEYCLOAK_CLIENT_ID=ai-bpms-client

# Application Settings
VITE_APP_NAME=AI-BPMS Client
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DEBUG_MODE=false

# Performance Settings
VITE_MAX_FILE_UPLOAD_SIZE=10485760
VITE_WEBSOCKET_RECONNECT_INTERVAL=5000
VITE_API_TIMEOUT=30000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_DEFAULT_LANGUAGE=en